<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Application icon -->
    <BitmapImage x:Key="ApplicationIconBitmapImage" UriSource="/OPTEL.UI.Desktop;component/Resources/Icons/optel.ico"/>
    <!-- Context menu icons -->
    <!-- Add/Create icon -->
    <Image x:Key="AddButtonImage" Source="/OPTEL.UI.Desktop;component/Resources/Icons/Controls/add.png"/>
    <BitmapImage x:Key="AddButtonBitmapImage" UriSource="/OPTEL.UI.Desktop;component/Resources/Icons/Controls/add.png"/>
    <!-- Remove/Delete icon -->
    <Image x:Key="RemoveButtonImage" Source="/OPTEL.UI.Desktop;component/Resources/Icons/Controls/remove.png"/>
    <BitmapImage x:Key="RemoveButtonBitmapImage" UriSource="/OPTEL.UI.Desktop;component/Resources/Icons/Controls/remove.png"/>
    <!-- Clone/Copy & Paste icon -->
    <Image x:Key="CloneButtonImage" Source="/OPTEL.UI.Desktop;component/Resources/Icons/Controls/clone.png"/>
    <BitmapImage x:Key="CloneButtonBitmapImage" UriSource="/OPTEL.UI.Desktop;component/Resources/Icons/Controls/clone.png"/>
    <!-- Save icon -->
    <Image x:Key="SaveButtonImage" Source="/OPTEL.UI.Desktop;component/Resources/Icons/Menu/save.png"/>
    <BitmapImage x:Key="SaveButtonBitmapImage" UriSource="/OPTEL.UI.Desktop;component/Resources/Icons/Menu/save.png"/>
</ResourceDictionary>
